<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:paddingLeft="10dp" android:paddingRight="10dp" android:shape="rectangle">
            <solid android:color="@color/backgroundButtonColorPushed" />
            <corners android:radius="50dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:paddingLeft="10dp" android:paddingRight="10dp" android:shape="rectangle">
            <solid android:color="@color/backgroundButtonColor" />
            <corners android:radius="50dp" />
        </shape>
    </item>
</selector>
